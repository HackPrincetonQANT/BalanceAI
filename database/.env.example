# Example environment variables for the database service
# Copy this file to `.env` and fill in real values (do NOT commit secrets).

# Database connection (example for SQLite)
DATABASE_URL=sqlite:///./dev.db

# Snowflake / external DB credentials (example placeholders)
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema

# Any other service keys
API_KEY=your_api_key_here
